{
    "name": "jellygnite/silverstripe-elemental-style",
    "description": "Dynamic styling for elemental blocks.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0 || ^5.0",
        "dnadesign/silverstripe-elemental": "*",
        "jellygnite/silverstripe-sliderfield": "dev-feature/CMS-5"
    },
    "autoload": {
        "psr-4": {
            "Jellygnite\\ElementalStyle\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ],
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/HeyImPhil/silverstripe-sliderfield.git"
        }        
    ]
}
